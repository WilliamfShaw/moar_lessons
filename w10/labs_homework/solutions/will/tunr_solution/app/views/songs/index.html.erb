<p><%= link_to "Add a song?", new_song_path %></p>
<% @songs.each do |song| %>
	<div class="single song">
		<h2><%= song.id %> | <%= song.title %></h2>
		<img src="<%= song.artwork %>">
		<p><%= song.artist.name %></p>
		<p><%= song.album %></p>
		<%= link_to "View Song", song_path(song) %>
	</div>
<% end %>