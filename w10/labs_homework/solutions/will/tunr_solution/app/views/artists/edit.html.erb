<%= form_for @artist do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>

	<%= f.label :photo_url %>
	<%= f.text_field :photo_url %>
	<br>

	<%= f.label :nationality %>
	<%= f.text_field :nationality %>
	<br>

	<%= f.submit  %>
<% end %>

<%= link_to "DELETE", artist_path(@artist), method: :delete %>