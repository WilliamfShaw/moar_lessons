<h1>New Entry</h1>

<%= form_for(@entry) do |f| %>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  <div class="field">
    <%= f.label :journal_id %><br>
    <%= f.collection_select(:journal_id, Journal.all, :id, :name) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<a href="/entries">Back</a>
